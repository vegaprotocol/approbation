{
  "name": "@vegaprotocol/approbation",
  "version": "4.6.0",
  "description": "Match Acceptance Criteria Codes with the tests that test them",
  "engine": ">= 18",
  "bin": "./bin/approbation.js",
  "scripts": {
    "pages-coverage": "node ./bin/approbation.js check-coverage --specs * --verbose --features=./test/test-data/features.json",
    "test:ci": "tape 'test/**/*.test.js'",
    "test": "tape 'test/**/*.test.js' | tap-arc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vegaprotocol/approbation.git"
  },
  "author": "Vega <hi@vega.xyz>",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/vegaprotocol/approbation/issues"
  },
  "homepage": "https://github.com/vegaprotocol/approbation#readme",
  "devDependencies": {
    "tap-arc": "0.3.5",
    "tape": "5.6.6"
  },
  "dependencies": {
    "fast-glob": "^3.3.0",
    "console-table-printer": "^2.10.0",
    "csv-parse": "^5.3.6",
    "find-root": "^1.1.0",
    "git-rev-sync": "3.0.2",
    "lodash.sortby": "^4.7.0",
    "minimist": "^1.2.5",
    "mustache": "^4.2.0",
    "picocolors": "^1.0.0",
    "rimraf": "^4.4.1"
  }
}
